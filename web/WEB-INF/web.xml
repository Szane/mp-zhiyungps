<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>CheckTruckServlet</servlet-name>
        <servlet-class>com.mp.servlet.CheckTruckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckTruckServlet</servlet-name>
        <url-pattern>/checkTruck</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HisTrackServlet</servlet-name>
        <servlet-class>com.mp.servlet.HisTrackServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HisTrackServlet</servlet-name>
        <url-pattern>/hisTrack</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LastLocationServlet</servlet-name>
        <servlet-class>com.mp.servlet.LastLocationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LastLocationServlet</servlet-name>
        <url-pattern>/lastLocation</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>QueryLicenseServlet</servlet-name>
        <servlet-class>com.mp.servlet.QueryLicenseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>QueryLicenseServlet</servlet-name>
        <url-pattern>/queryLicense</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>OC3MD5UtilServlet</servlet-name>
        <servlet-class>com.mp.servlet.OC3MD5UtilServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OC3MD5UtilServlet</servlet-name>
        <url-pattern>/paySign</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>BMWMD5UtilServlet</servlet-name>
        <servlet-class>com.mp.servlet.BMWMD5UtilServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BMWMD5UtilServlet</servlet-name>
        <url-pattern>/bmwPaySign</url-pattern>
    </servlet-mapping>
</web-app>